    "license": "MIT",
    "scripts": {
        "build-sass": "sass src/public/css/main.scss build/public/css/main.css",
        "build-ts": "tsc",
        "build": "npm run build-ts && npm run lint",
        "clean": "rm -rf build && rm -f tsconfig.tsbuildinfo",
        "debug": "npm run build && npm run watch-debug",
        "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
        "serve-debug": "nodemon --inspect build/server.js",
        "serve": "node build/server.js",
        "start": "npm run serve",
        "test": "jest --forceExit --coverage --verbose",
        "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
        "watch-node": "nodemon build/server.js",
        "watch-test": "npm run test -- --watchAll",
        "watch-ts": "tsc -w",
        "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\""
    },
    "dependencies": {
        "@influxdata/influxdb-client": "^1.33.2",
        "async": "^3.2.5",
        "axios": "^1.6.3",
        "body-parser": "^1.20.2",
        "compression": "^1.7.4",
        "dotenv": "^16.3.1",
        "errorhandler": "^1.5.1",
        "express": "^4.18.2",
        "express-validator": "^7.0.1",
        "js-yaml": "^4.1.0",
        "lodash": "^4.17.21",
        "lusca": "^1.7.0",
        "mqtt": "^5.10.1",
        "node-dht-sensor": "^0.4.3",
        "node-schedule": "^2.1.1",
        "winston": "^3.11.0"
    },
    "devDependencies": {
        "@types/async": "^3.2.24",
        "@types/body-parser": "^1.19.5",
        "@types/chai": "^4.3.11",
        "@types/compression": "^1.7.5",
        "@types/errorhandler": "^1.5.3",
        "@types/eslint": "^8.56.0",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.11",
        "@types/js-yaml": "^4.0.9",
        "@types/lodash": "^4.14.202",
        "@types/lusca": "^1.7.4",
        "@types/node": "^20.10.6",
        "@types/node-dht-sensor": "^0.4.2",
        "@types/node-schedule": "^2.1.5",
        "@types/request": "^2.48.12",
        "@types/request-promise": "^4.1.51",
        "@types/supertest": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "^6.17.0",
        "@typescript-eslint/parser": "^6.17.0",
        "chai": "^5.0.0",
        "concurrently": "^8.2.2",
        "eslint": "^8.56.0",
        "jest": "^29.7.0",
        "nodemon": "^3.0.2",
        "shelljs": "^0.8.5",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
    },
    "engines": {
        "node": ">=16.0.0"
    }
}
