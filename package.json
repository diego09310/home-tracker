{
    "name": "home-tracker",
    "version": "0.1.0",
    "description": "Service to track parameters from different sources using ticktock as TSDB",
    "author": {
        "name": "Diego AvendaÃ±o Peces",
        "email": "dap09310@gmail.com",
        "url": "https://diego09310.github.io/"
